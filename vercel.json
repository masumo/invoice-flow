{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_CONTRACT_ADDRESS": "@vite_contract_address",
    "VITE_RPC_URL": "@vite_rpc_url",
    "VITE_CHAIN_ID": "@vite_chain_id",
    "VITE_NETWORK_NAME": "@vite_network_name"
  }
}